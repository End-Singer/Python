# 定义变量存储布尔类型的数据
bool_1 = True
bool_2 = False
print(f"bool_1变量的内容是{bool_1},类型是：{type(bool_1)}")
print(f"bool_2变量的内容是{bool_2},类型是：{type(bool_2)}")
# 比较运算符 == != > < >= <=
name1 = "博丽灵梦"
name2 = "雾雨魔理沙"
print(f"结果是：{name1 == name2}")
print(f"结果是：{name1 != name2}")

num1 = 1
num2 = 5
print(f"1 > 5的结果是：{num1 > num2}")
print(f"1 <= 5的结果是：{num1 <= num2}")


